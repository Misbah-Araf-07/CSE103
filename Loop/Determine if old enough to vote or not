#include <stdio.h>
void isEligibleOrNot(int x);

int main()
{
    int n,a,counter;

    n=10;
    counter=1;
    while (counter<=n)
    {
        printf("Please Enter a Number: ");
        scanf("%d",&a);
        isEligibleOrNot(a);
        counter= counter+1;
    }

    return 0;
    printf("\n");
}


void isEligibleOrNot(int age)
{
    if (age>=18)
        printf("Eligible for voting\n\n");
    else
        printf("Not Eligible for voting\n\n");
}
