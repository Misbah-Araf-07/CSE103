#include <stdio.h>
int main ()
{
    {
        int a=65;
        printf("Upper Case: ");
        for ( ; a<=90; a++)
            if(a>=90)
                printf(" ");
            else
                printf("%c",a);
    }
    printf(" \n");



    {
        int a=97;
        printf("Lower Case: ");
        for ( ; a<=122 ; a++)
            if (a>122)
                printf(" ");
            else
                printf("%c",a);

    }
    printf(" \n");
    return 0 ;
}
