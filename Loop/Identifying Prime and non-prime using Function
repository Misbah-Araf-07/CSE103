#include <stdio.h>
#include <stdbool.h>

bool isPrime(int x);
int main ()
{
    int n;
    scanf("%d", &n);
    if ( isPrime(n))
        printf("%d is prime\n",n);
    else
        printf("%d is non prime\n",n);
    return 0;

}

bool isPrime(int x)
{
    if (x==1)
          return false ;
    int i =2;
    while (i<x)
         {
          if (x%i==0)
          return false ;
        i++;
         }
      return true;
}
